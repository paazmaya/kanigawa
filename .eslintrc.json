{
  "extends": "paazmaya",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "arrowFunctions": true,
    "classes": true,
    "modules": true,
    "templateStrings": true,
    "jsx": true
  },
  "globals": {
    "document": false,
    "Tinbe": true,
    "Headers": false
  }
}
