{
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-spacing": 2,
    "arrow-body-style": [2, "always"],
    "arrow-parens":  [2, "always"],
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": [2, "stroustrup"],
    "callback-return": 2,
    "camelcase": 0,
    "comma-dangle": 2,
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "complexity": [1, 12],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 2,
    "consistent-this": [2, "_self"],
    "constructor-super": 2,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 1,
    "eol-last": 2,
    "eqeqeq": 2,
    "func-names": 2,
    "func-style": [1, "expression"],
    "generator-star-spacing": 2,
    "global-require": 2,
    "guard-for-in": 2,
    "handle-callback-err": 2,
    "id-length": 2,
    "id-match": 2,
    "indent": [1, 2, {"VariableDeclarator": 1}],
    "init-declarations": 0,
    "jsx-quotes": 2,
    "key-spacing": 2,
    "linebreak-style": 2,
    "lines-around-comment": 2,
    "max-depth": [1, 4],
    "max-len": [1, 120, 4],
    "max-nested-callbacks": [1, 2],
    "max-params": [1, 3],
    "max-statements": [1, 10],
    "new-cap": 1,
    "new-parens": 2,
    "newline-after-var": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-arrow-condition": 2,
    "no-bitwise": 1,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-label": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 1,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "functions"],
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 1,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-magic-numbers": [2, {"ignore": [-1, 0, 1]}],
    "no-mixed-requires": [1, true],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-native-reassign": 2,
    "no-negated-condition": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 1,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-path-concat": 2,
    "no-plusplus": 1,
    "no-process-env": 1,
    "no-process-exit": 1,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-modules": 1,
    "no-restricted-syntax": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 1,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-sync": 1,
    "no-ternary": 1,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 1,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, "local"],
    "no-use-before-define": 1,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "object-curly-spacing": [1, "always"],
    "object-shorthand": 1,
    "one-var": 0,
    "operator-assignment": [2, "always"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": [1, "never"],
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-reflect": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quote-props": [1, "as-needed"],
    "quotes": [2, "single"],
    "radix": 2,
    "react/display-name": [2, {"acceptTranspilerName": true}],
    "react/forbid-prop-types": [2, {"forbid": ["object", "array"]}],
    "react/jsx-boolean-value": 1,
    "react/jsx-closing-bracket-location": [1, "after-props"],
    "react/jsx-curly-spacing": [2, "always", {"allowMultiline": true}],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-key": 1,
    "react/jsx-max-props-per-line": [2, {"maximum": 1}],
    "react/jsx-no-bind": 1,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-literals": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-pascal-case": 2,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 1,
    "react/no-did-mount-set-state": 1,
    "react/no-did-update-set-state": 1,
    "react/no-direct-mutation-state": 1,
    "react/no-multi-comp": 2,
    "react/no-set-state": 1,
    "react/no-unknown-property": 1,
    "react/prefer-es6-class": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 1,
    "react/require-extension": 1,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "react/wrap-multilines": 1,
    "require-jsdoc": 1,
    "require-yield": 2,
    "semi": 2,
    "semi-spacing": [2, {"before": false, "after": true}],
    "sort-vars": 0,
    "space-after-keywords": 1,
    "space-before-blocks": 1,
    "space-before-function-paren": 2,
    "space-before-keywords": 2,
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": 1,
    "spaced-comment": [1, "always"],
    "strict": [2, "global"],
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "vars-on-top": 1,
    "wrap-iife": [2, "inside"],
    "wrap-regex": 2,
    "yoda": [2, "never"]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "classes": true,
    "modules": true,
    "templateStrings": true,
    "jsx": true
  },
  "globals": {
    "document": false,
    "Tinbe": true,
    "Headers": false
  }
}
