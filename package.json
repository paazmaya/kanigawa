{
  "name": "kanigawa",
  "version": "0.1.0",
  "description": "Electron based desktop application for setting geotags for images",
  "main": "server.js",
  "scripts": {
    "start-app": "electron server.js",
    "start": "npm run serve | npm run dev",
    "test": "npm run lint",
    "lint": "eslint server.js src/*.jsx src/jsx/*.jsx",
    "pages": "npm run build && npm run move-bundle",
    "serve": "http-server -p 9980 ./",
    "serve-https": "http-server -p 9980 --ssl --cert certificates/server.crt --key certificates/server.key ./",
    "build": "webpack --config webpack.config.js --progress --optimize-minimize",
    "move-bundle": "mkdir -p pages/assets && mv build/* pages/assets/",
    "dev": "webpack-dev-server --progress --colors --port 9981",
    "r-build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "engines": {
    "node": ">=6.9.5",
    "npm": ">=3.5.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (https://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "homepage": "https://github.com/paazmaya/kanigawa",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^4.19.0",
    "eslint-config-paazmaya": "^4.3.0",
    "eslint-plugin-react": "^7.7.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-server": "^0.11.1",
    "image-webpack-loader": "^4.2.0",
    "imports-loader": "^0.8.0",
    "is-image": "^2.0.0",
    "leaflet": "^1.3.1",
    "postcss": "^6.0.20",
    "postcss-loader": "^2.1.2",
    "react": "^16.2.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.9.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "exiv2": "^0.6.3"
  }
}
