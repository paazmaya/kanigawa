{
  "name": "kanigawa",
  "version": "0.1.0",
  "description": "Electron based desktop application for setting geotags for images",
  "main": "server.js",
  "scripts": {
    "start-app": "electron server.js",
    "start": "npm run serve | npm run dev",
    "test": "npm run lint",
    "lint": "eslint -c .eslintrc.json server.js src/*.jsx src/jsx/*.jsx",
    "pages": "npm run build && npm run move-bundle",
    "serve": "http-server -p 9980 ./",
    "serve-https": "http-server -p 9980 --ssl --cert certificates/server.crt --key certificates/server.key ./",
    "build": "webpack --config webpack.config.js --progress --optimize-minimize",
    "move-bundle": "mkdir -p pages/assets && mv build/* pages/assets/",
    "dev": "webpack-dev-server --progress --colors --port 9981",
    "r-build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "engines": {
    "node": ">=6.9.5",
    "npm": ">=3.5.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (https://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "homepage": "https://github.com/paazmaya/kanigawa",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.0",
    "electron-prebuilt": "^1.4.8",
    "eslint": "^3.10.2",
    "eslint-config-paazmaya": "^3.1.0",
    "eslint-plugin-react": "^6.7.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "^0.6.5",
    "is-image": "^1.0.1",
    "leaflet": "^1.0.2",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.1.1",
    "react": "^15.4.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.4.1",
    "react-leaflet": "^1.0.0",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "exiv2": "^0.6.2"
  }
}
