{
  "name": "kanigawa",
  "productName": "kanigawa",
  "description": "Electron based desktop application for setting geotags for images",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "main": "server.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag",
    "organize",
    "pictures",
    "location"
  ],
  "homepage": "./",
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (https://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/leaflet": "1.5.1",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-leaflet": "2.4.0",
    "leaflet": "1.5.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-leaflet": "2.4.0",
    "react-scripts": "3.1.1",
    "redux-zero": "5.1.0",
    "source-map-explorer": "2.0.1",
    "typescript": "3.6.3",
    "snyk": "^1.227.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.39",
    "@electron-forge/maker-deb": "6.0.0-beta.39",
    "@electron-forge/maker-rpm": "6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "6.0.0-beta.39",
    "@electron-forge/maker-zip": "6.0.0-beta.39",
    "electron": "5.0.6",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "4.0.0",
    "is-image": "3.0.0",
    "ws": "7.1.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html build/treemap.html",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:publish": "electron-forge publish",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "darwinDarkModeSupport": true,
        "icon": "kanigawa-icon",
        "appCategoryType": "public.app-category.photography"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kanigawa_electron_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 edge version",
    "last 2 electron version",
    "last 2 firefox version",
    "last 2 safari version"
  ],
  "snyk": true
}
