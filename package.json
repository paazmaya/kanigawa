{
  "name": "kanigawa",
  "version": "0.1.0",
  "description": "Electron based desktop application for setting geotags for images",
  "main": "server.js",
  "scripts": {
    "start-app": "electron server.js",
    "start": "npm run serve | npm run dev",
    "test": "npm run lint",
    "lint": "eslint -c .eslintrc.json index.js src/*.jsx src/jsx/*.jsx",
    "pages": "npm run build && npm run move-bundle",
    "serve": "http-server -p 9980 --ssl --cert certificates/server.crt --key certificates/server.key ./",
    "build": "webpack --config webpack.config.js --progress --optimize-minimize",
    "move-bundle": "mkdir -p pages/assets && mv build/* pages/assets/",
    "dev": "webpack-dev-server --progress --colors --port 9981"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (http://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "homepage": "https://github.com/paazmaya/kanigawa",
  "devDependencies": {
    "autoprefixer": "^6.1.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.0",
    "electron-prebuilt": "^0.35.1",
    "eslint": "^1.10.1",
    "eslint-plugin-react": "^3.10.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "http-server": "^0.8.5",
    "image-webpack-loader": "^1.6.2",
    "imports-loader": "^0.6.5",
    "is-image": "^1.0.1",
    "leaflet": "^0.7.7",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.3",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dom": "^0.14.3",
    "react-leaflet": "^0.9.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "exiv2": "^0.6.2"
  }
}
