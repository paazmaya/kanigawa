{
  "name": "kanigawa",
  "version": "0.1.0",
  "description": "Electron based desktop application for setting geotags for images",
  "main": "server.js",
  "scripts": {
    "start-app": "electron server.js",
    "start": "npm run serve | npm run dev",
    "test": "npm run lint",
    "lint": "eslint -c .eslintrc.json server.js src/*.jsx src/jsx/*.jsx",
    "pages": "npm run build && npm run move-bundle",
    "serve": "http-server -p 9980 ./",
    "serve-https": "http-server -p 9980 --ssl --cert certificates/server.crt --key certificates/server.key ./",
    "build": "webpack --config webpack.config.js --progress --optimize-minimize",
    "move-bundle": "mkdir -p pages/assets && mv build/* pages/assets/",
    "dev": "webpack-dev-server --progress --colors --port 9981",
    "r-build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=3.5.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (http://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "homepage": "https://github.com/paazmaya/kanigawa",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.36.10",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^4.2.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "is-image": "^1.0.1",
    "leaflet": "^0.7.7",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.8.1",
    "react": "^0.14.7",
    "react-dnd": "^2.1.2",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^0.14.7",
    "react-leaflet": "^0.10.1",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "exiv2": "^0.6.2"
  }
}
