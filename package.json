{
  "name": "kanigawa",
  "productName": "kanigawa-electron-forge",
  "description": "Electron based desktop application for setting geotags for images",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/paazmaya/kanigawa.git"
  },
  "main": "server.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "keywords": [
    "image",
    "gps",
    "geo",
    "geotag",
    "organize",
    "pictures",
    "location"
  ],
  "author": "Juga Paazmaya <paazmaya@yahoo.com> (https://paazmaya.fi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paazmaya/kanigawa/issues"
  },
  "homepage": "https://github.com/paazmaya/kanigawa",
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/leaflet": "1.4.4",
    "@types/node": "12.0.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-leaflet": "2.2.1",
    "leaflet": "1.5.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-leaflet": "2.3.0",
    "react-scripts": "3.0.1",
    "redux-zero": "5.0.2",
    "source-map-explorer": "2.0.0",
    "typescript": "3.5.1"
  },
  "devDependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "@electron-forge/cli": "6.0.0-beta.39",
    "@electron-forge/maker-deb": "6.0.0-beta.39",
    "@electron-forge/maker-rpm": "6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "6.0.0-beta.39",
    "@electron-forge/maker-zip": "6.0.0-beta.39",
    "electron": "5.0.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html build/treemap.html",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "electron-forge start",
    "electron:package": "electron-forge package",
    "electron:make": "electron-forge make",
    "electron:publish": "electron-forge publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kanigawa_electron_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
